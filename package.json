{
  "name": "kuwomusicapi",
  "version": "1.0.0",
  "description": "Egg for 酷我音乐API;酷我音乐API Node.js版",
  "private": true,
  "keywords": [
    "酷我音乐API Node.JS版",
    "音乐API",
    "酷我音乐API"
  ],
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --port 7002 --title=egg-server-server-egg",
    "stop": "egg-scripts stop --title=egg-server-server-egg",
    "docs": "docsify serve ./docs",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": " tsc -p tsconfig.json",
    "build": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "docsify": "^4.13.0",
    "egg": "^3.15.0",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.17.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "autod": "^3.1.2",
    "autod-egg": "^1.1.0",
    "docsify-cli": "^4.4.4",
    "egg-bin": "^6.1.2",
    "egg-ci": "^2.2.0",
    "egg-mock": "^5.10.6",
    "eslint": "^8.36.0",
    "eslint-config-egg": "^12.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsdoc": "^40.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QiuYaohong/kuwoMusicApi.git"
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "qyhqiu",
  "license": "ISC"
}
